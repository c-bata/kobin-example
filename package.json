{
  "name": "kobin-example",
  "version": "0.0.1",
  "description": "Example application in Kobin python web-framework.",
  "scripts": {
    "tsc": "tsc -p .",
    "browserify": "browserify ./static/ts/main.js -o ./public/static/js/bundle.js",
    "sass": "node-sass --include-path static/scss static/scss/style.scss public/static/css/style.css",
    "collectimg": "cp -r ./static/img/ ./public/static/img",
    "build": "npm run tsc && npm run browserify && npm run sass && npm run collectimg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c-bata/kobin-example.git"
  },
  "keywords": [],
  "author": "c-bata",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c-bata/kobin-example/issues"
  },
  "homepage": "https://github.com/c-bata/kobin-example#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.15",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "typescript": "^1.8.10",
    "zone.js": "^0.6.11"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "node-sass": "^3.4.2"
  }
}
