{
  "name": "kobin-todo",
  "version": "0.0.1",
  "description": "Todo application in Kobin python web-framework.",
  "scripts": {
    "dir": "mkdir -p ./public/img ./public/js ./public/css",
    "tsc": "tsc -p .",
    "browserify": "browserify ./ts/main.js -o ./public/js/bundle.js",
    "stylus": "stylus ./stylus/style.styl -o ./public/css/style.css",
    "img": "cp -r ./img/ ./public/img",
    "build": "npm run dir && npm run tsc && npm run browserify && npm run stylus && npm run img"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c-bata/kobin-todo.git"
  },
  "keywords": [],
  "author": "c-bata",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c-bata/kobin-example/issues"
  },
  "homepage": "https://github.com/c-bata/kobin-example#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.15",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "typescript": "^2.1.4",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "stylus": "^0.54.5"
  }
}
